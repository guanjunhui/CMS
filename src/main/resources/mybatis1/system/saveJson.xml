<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="saveJson">
	<insert id="save"  parameterType="pd">
	insert into WEB_ADDRESS_QUERY(ID,PROVINCE,CITY,BRAND,MY_NAME,TEL,ADDRESS)
	values(#{ID},#{PROVINCE},#{CITY},#{BRAND},#{MY_NAME},#{TEL},#{ADDRESS})
	
	</insert>

</mapper>